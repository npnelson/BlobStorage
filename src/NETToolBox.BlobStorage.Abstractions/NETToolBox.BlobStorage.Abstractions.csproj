<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Nullable>enable</Nullable>
    <ProjectGuid>687057b8-e38e-459d-90c8-d2ac76705c11</ProjectGuid>
    <RepositoryUrl>https://github.com/npnelson/BlobStorage</RepositoryUrl>
    <Description>Abstraction layer for blob storage</Description>
    <Authors>npnelson</Authors>
    <PackageProjectUrl>https://github.com/npnelson/BlobStorage</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  
  <ItemGroup>
    <SonarQubeSetting Include="sonar.coverage.exclusions">
      <Value>**</Value>
    </SonarQubeSetting>   
  </ItemGroup>
</Project>
